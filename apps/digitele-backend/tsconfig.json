{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "composite": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "declaration": true
  }
}